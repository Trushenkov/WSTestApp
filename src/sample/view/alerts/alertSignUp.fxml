            <font>
                <Font name="Microsoft Sans Serif" size="18.0"/>
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Label fx:id="labelSignUp" layoutX="115.0" layoutY="14.0" prefHeight="48.0" prefWidth="170.0" text="Уведомление"
               textFill="#796c6c">
            <font>
                <Font name="MS Reference Sans Serif" size="24.0"/>
            </font>
        </Label>
        <Label layoutX="50.0" layoutY="88.0" text="Вы успешно зарегистрировались в системе"/>
    </children>
</AnchorPane>
