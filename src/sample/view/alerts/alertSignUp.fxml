<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="sample.controllers.alerts.AlertController">
    <children>
        <Button fx:id="buttonCloseAlert" layoutX="85.0" layoutY="147.0" mnemonicParsing="false" prefHeight="50.0"
