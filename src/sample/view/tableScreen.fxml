<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="700.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="sample.controllers.OrderWindowController">
    <children>
        <AnchorPane id="anchor" prefHeight="200.0" prefWidth="1100.0">
            <children>
                <ImageView fx:id="logo" fitHeight="186.0" fitWidth="191.0" layoutX="7.0" layoutY="7.0"
                           pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/nba-logo.png"/>
                    </image>
                </ImageView>
                <Button fx:id="buttonExit" layoutX="813.0" layoutY="75.0" mnemonicParsing="false" prefHeight="50.0"
                        prefWidth="230.0" text="Выход" textFill="#796c6c">
                    <cursor>
                        <Cursor fx:constant="HAND"/>
                    </cursor>
                    <font>
                        <Font name="Microsoft Sans Serif" size="18.0"/>
                    </font>
                </Button>
            </children>
        </AnchorPane>
        <TableView layoutX="124.0" layoutY="294.0" prefHeight="367.0" prefWidth="852.0">
            <columns>
                <TableColumn prefWidth="123.0" text="C1"/>
                <TableColumn prefWidth="98.0" text="C2"/>
                <TableColumn prefWidth="133.0" text="Column X"/>
                <TableColumn prefWidth="159.0" text="Column X"/>
                <TableColumn prefWidth="163.0" text="Column X"/>
                <TableColumn prefWidth="175.0" text="Column X"/>
            </columns>
        </TableView>
        <Label fx:id="labelWelcome" layoutX="443.0" layoutY="217.0" prefHeight="48.0" prefWidth="214.0"
               text="Список заказов" textFill="#796c6c">
            <font>
                <Font name="MS Reference Sans Serif" size="24.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
